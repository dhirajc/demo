<div class="row">
<%#= raise @looks.inspect %>
	<% @looks.each do |look| %>
	<%# raise look.product_id.inspect %>
	<% @p =  Product.find(look.product_id) %>
	<%#= raise @p.inspect %>
<div class="col-sm-6 col-md-4">
<div class="thumbnail">
	<%#= image_tag look.product.avatar.url(:thumb) %>
	<%= image_tag @p.avatar.url(:thumb) %>
<div class="caption">
<h3><%= look.name %></h3>

<p>
	<%= link_to 'Show',  look_path(look) , :class => "btn btn-primary"%>
	<%= link_to 'Edit', edit_look_path(look), :class => "btn btn-primary" %>
	<%= link_to 'Delete',look_path(look), method: :delete, data: { confirm: 'Are you sure you want to delete this alert?' }, :class => "btn btn-default" %>
</p> 
</p> 
</div>
</div>
</div>
<% end %>
</div>
<%= link_to 'Customize Products', products_path %>