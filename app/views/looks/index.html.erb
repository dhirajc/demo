<div class="row">
<%#= raise @looks.inspect %>
	<% @looks.each do |f| %>
	<%#= raise .inspect %>
	<%#= raise f.product_id.inspect %>
	<% @p =  Product.find(f.product_id) %>
	<%#= raise @p.inspect %>
<div class="col-sm-6 col-md-4">
<div class="thumbnail">
	<%= image_tag @p.avatar.url(:thumb) %>
<div class="caption">
<h3><%= f.name %></h3>

<p>
	<%= link_to 'Show',  look_path(f) , :class => "btn btn-primary"%>
	<%= link_to 'Edit', edit_look_path(f), :class => "btn btn-primary" %>
	<%= link_to 'Delete',look_path(f),:confirm => 'Are you sure?', :method => :delete , :class => "btn btn-default" %>
 
</p> 
</p> 
</div>
</div>
</div>
<% end %>
</div>