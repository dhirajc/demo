
<%= form_for @product ,  :url  => admin_products_path, html: { multipart: true }  do |f| %>
 
 <li> 
 	<%= f.label :name %>
  	<%= f.text_field :name%>
 </li>
 
 <li>
  <%= f.label :description %>
  <%= f.text_area :description %>
 </li></br>
 
 <li> <%= f.file_field :avatar %></li><br/>
<%= f.submit "Submit" %>
<% end %>